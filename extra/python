# Python stuff

# python-install-scripts contains executables installed using python
# (easy_install, etc)
PATH="$PATH:/Users/gabe/bin/python-install-scripts"
# For pip-installed stuff
PATH="$PATH:$(brew --prefix python)/bin"
export PATH

export PYTHONSTARTUP=~/.python_startup
# =<command> is like $(which command)
[[ -x $(which pip 2>/dev/null) ]] && eval "`pip completion --zsh`"
